    "def split_address(df, col_name):\n",
    "    address = dict()\n",
    "    series = df.index.to_list()\n",
    "    for i in series:\n",
    "        broken_address = df[col_name][i].split(\",\")\n",
    "        broken2 = broken_address[-2].split()\n",
    "        city = broken_address[-3]\n",
    "        province = broken2[0]\n",
    "        zipcode = broken2[1]\n",
    "        country = broken_address[-1]\n",
    "        if len(broken_address[:-3]) !=0:\n",
    "            if len(broken_address[:-3]) >1:\n",
    "                street_address = broken_address[:-3][-1]\n",
    "            else:\n",
    "                street_address = broken_address[:-3][0]\n",
    "        else:        \n",
